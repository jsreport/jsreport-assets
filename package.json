{
  "name": "jsreport-assets",
  "version": "0.7.1",
  "description": "jsreport extension embedding static assets like fonts or helpers into the templates",
  "scripts": {
    "start": "jsreport-studio-start",
    "run": "npm run build && jsreport-studio-start --run-only",
    "test": "mocha test --timeout 5000 --exit && standard",
    "test:watch": "mocha test --watch",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-assets",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-assets.git"
  },
  "standard": {
    "ignore": [
      "main.js",
      "helpers.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "keywords": [
    "jsreport",
    "assets"
  ],
  "license": "MIT",
  "dependencies": {
    "async-replace": "1.0.1",
    "bluebird": "3.5.1",
    "etag": "1.8.1",
    "js-string-escape": "1.0.1",
    "mime": "2.2.0",
    "minimatch": "3.0.4",
    "nanoid": "1.0.1",
    "strip-bom-buf": "1.0.0"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.17.0",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-babel": "4.1.2",
    "in-publish": "2.0.0",
    "is-number-like": "1.0.8",
    "jsreport-core": "github:jsreport/jsreport-core#2.0.0-beta",
    "jsreport-express": "github:jsreport/jsreport-express#2.0.0-beta",
    "jsreport-jsrender": "1.0.2",
    "jsreport-scripts": "github:jsreport/jsreport-scripts#2.0.0-beta",
    "jsreport-studio-dev": "1.1.2",
    "jsreport-templates": "github:jsreport/jsreport-templates#2.0.0-beta",
    "mocha": "5.0.0",
    "react-copy-to-clipboard": "4.3.0",
    "should": "13.2.1",
    "standard": "10.0.3",
    "supertest": "3.0.0"
  },
  "engines": {
    "node": ">=8.0"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
